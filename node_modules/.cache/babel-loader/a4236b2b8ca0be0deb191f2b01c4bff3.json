AACA,UAAA,WAAA,GAAA,KAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA,UAAA,WAAA,GAAA,CAAA,EACA,KAAA,GAAA,WAAA,GAAA,CAAA;AACA,UAAA,CAAA,KAAA,QAAA,CAAA,KAAA,CAAA,KAAA,CAAA,EACA,OAAA,IAAA;AACA,aAAA,KAAA;AACA;AAZA,IAZA;AA2BA,EAAA,OAAA,kCACA,YAAA,CAAA,CAAA,aAAA,EAAA,aAAA,EAAA,QAAA,CAAA,CADA;AAEA;AACA,IAAA,MAHA,oBAGA;AACA,WAAA,WAAA,CAAA,KAAA,QAAA,EADA,CAEA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AAEA,KARA;AASA;AACA,IAAA,QAVA,sBAUA;AACA,WAAA,QAAA,CAAA,KAAA,CAAA,IAAA,CAAA,EAAA;AACA,KAZA;AAaA;AACA,IAAA,WAdA,uBAcA,aAdA,EAcA;AACA,UAAA,aAAA,GAAA,KAAA,QAAA,CAAA,KAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA,KAAA;AAAA,eAAA,KAAA,KAAA,aAAA;AAAA,OAAA,CAAA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,aAAA;AACA,KAjBA;AAkBA,IAAA,OAlBA,mBAkBA,CAlBA,EAkBA;AACA,MAAA,CAAA,CAAA,cAAA,GADA,CAEA;;AACA,WAAA,QAAA,CAAA,IAAA,GAAA,EAAA;AACA,WAAA,QAAA,CAAA,KAAA,GAAA,CAAA,EAAA,CAAA;AACA,WAAA,QAAA,CAAA,OAAA,GAAA,EAAA;AACA,WAAA,QAAA,CAAA,QAAA,GAAA,EAAA;AAEA;AA1BA;AA3BA,CAAA","sourcesContent":["<template>\r\n  <div class=\"Create\">\r\n   <v-card\r\n    max-width=\"500\"\r\n    class=\"bgd2 mx-auto mt-5\"\r\n   >\r\n    \r\n    <v-app-bar\r\n    class=\"pnl\"\r\n    >\r\n       <v-btn  color=\"dark\" icon text :to=\"{name : 'Primera'}\">\r\n       <v-icon>keyboard_backspace</v-icon>\r\n       </v-btn>\r\n       <v-toolbar-title class=\"text-center mx-2\">Smoothie Match</v-toolbar-title>   \r\n     </v-app-bar>\r\n\r\n        <v-container>\r\n             <v-form v-model=\"valid\">\r\n               <!-- agg smoothie name -->\r\n               <v-row \r\n                 class=\"centro\"                              \r\n                 >\r\n                  <v-col cols=\"10\" sm=\"10\">\r\n                    <p class=\"font-weight-bold\">Smoothie Name:</p>\r\n                      <v-text-field\r\n                        color=\"green\"\r\n                        v-model=\"smoothie.name\"\r\n                        :rules=\"nameRules\"\r\n                        label=\"Name\"\r\n                        required\r\n                       ></v-text-field>\r\n                  </v-col>\r\n              </v-row>\r\n              <!-- agg smoothie fruit -->\r\n                <v-row \r\n                   class=\"centro\"                  \r\n                 >\r\n                   <v-col cols=\"8\" sm=\"8\">\r\n                     <p class=\"font-weight-bold black--text\">Fruits:</p>\r\n                       <v-select\r\n                        v-model=\"smoothie.fruit[0]\"\r\n                        :rules=\"nameRules\"\r\n                        :items=\"fruits\"\r\n                        label=\"Fruits\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                    <v-col cols=\"2\" sm=\"2\" align-self=\"center\">\r\n                      <v-btn :disabled=\"disabledAddFruit\" color=\"green\" icon text @click=\"addFruit\">\r\n                        <v-icon>add_box</v-icon>\r\n                      </v-btn>\r\n                    </v-col>\r\n                </v-row>\r\n                <!-- other fruits -->\r\n                <div v-for=\"(item, i) in smoothie.fruit\" :key=\"`smoothie-fruit-${i}`\">\r\n                    <v-row \r\n                   class=\"centro\"\r\n                   \r\n                   v-if=\"i > 0\"\r\n                    >\r\n                   <v-col cols=\"8\" sm=\"8\">\r\n                     <p class=\"font-weight-bold\">Fruits:</p>\r\n                       <v-select\r\n                        v-model=\"smoothie.fruit[i]\"\r\n                        :items=\"fruits\"\r\n                        label=\"Fruits\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                    <v-col cols=\"2\" sm=\"2\" align-self=\"center\">\r\n                      <v-btn color=\"red\" icon text @click=\"removeFruit(i)\">\r\n                        <v-icon>clear</v-icon>\r\n                      </v-btn>\r\n                    </v-col>\r\n                </v-row>\r\n                  </div>\r\n                  <!-- add smoothie liquids -->\r\n                <v-row \r\n                   class=\"centro\"                   \r\n                 >\r\n                   <v-col cols=\"10\" sm=\"10\">\r\n                     <p class=\"font-weight-bold\">Liquids:</p>\r\n                       <v-select\r\n                       color=\"green\"\r\n                        v-model=\"smoothie.liquids\"\r\n                        :rules=\"nameRules\"\r\n                        :items=\"liquids\"\r\n                        label=\"Liquids\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                </v-row>   \r\n                <!-- add proteins smoothie  -->\r\n                <v-row \r\n                   class=\"centro\"                 \r\n                 >\r\n                   <v-col cols=\"10\" sm=\"10\">\r\n                     <p class=\"font-weight-bold\">Proteins:</p>\r\n                       <v-select\r\n                        v-model=\"smoothie.proteins\"\r\n                        :rules=\"nameRules\"\r\n                        :items=\"proteins\"\r\n                        label=\"Proteins\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                </v-row>  \r\n                <!-- add smoothie taste   -->\r\n                <v-row \r\n                   class=\"ml-6\"                \r\n                 >\r\n                  <v-icon color=\"red\" >whatshot</v-icon>\r\n                   <v-col cols=\"4\" sm=\"3\">\r\n                     <p class=\"font-weight-bold\">Taste:</p>                  \r\n                       <v-text-field\r\n                         value=\"Taste\"\r\n                         :rules=\"nameRules\"\r\n                         suffix=\"%\"\r\n                         v-model=\"smoothie.taste\"\r\n                         outlined\r\n                         type=\"number\"\r\n                      >\r\n                      </v-text-field>                     \r\n                   </v-col>\r\n                   <v-col cols=\"10\" sm=\"12\" class=\"text-center\">\r\n                     <!-- submit button -->\r\n                     <v-btn class=\"ma-2\"  color=\"indigo white--text\" @click=\"submit\" v-if=\"valid\">Save it!</v-btn>\r\n                     <!-- clean button -->\r\n                     <v-btn class=\"ma-2\"  color=\"red white--text\"  @click=\"onReset\" >Clean!</v-btn>\r\n                   </v-col>\r\n                </v-row>    \r\n                \r\n               </v-form>\r\n        </v-container>\r\n     </v-card>\r\n   </div>\r\n  \r\n</template>\r\n\r\n<script>\r\n   \r\n   import {mapState, mapMutations} from 'vuex';\r\n   import styles from '@/assets/background.css'\r\n \r\n  export default {\r\n    name: 'Create',\r\n   \r\n    data: () => ({\r\n  // check if all data has information\r\n      valid: true,\r\n        nameRules: [\r\n        v => !!v || 'Please is required',\r\n            ],\r\n      \r\n      \r\n    }),\r\n    computed:{\r\n      ...mapState(['fruits','proteins','liquids', 'smoothies', 'smoothie']),\r\n      \r\n// the add fruit button does not activate\r\n      disabledAddFruit() {\r\n      let index = 0;   \r\n      const countFruits = this.smoothie.fruit.length;\r\n      if(countFruits > 0)\r\n        index = countFruits - 1;\r\n      if(!this.smoothie.fruit[index])\r\n        return true;\r\n      return false;\r\n    }\r\n    },\r\n    \r\n    methods:{\r\n      ...mapMutations(['aggSmoothie', 'obtSmoothie', 'volver']),\r\n// submit new smoothie\r\n      submit(){\r\n        this.aggSmoothie(this.smoothie)\r\n// back home\r\n        this.$router.push('/')\r\n        \r\n      },\r\n// add fruit new\r\n      addFruit() {\r\n      this.smoothie.fruit.push('');\r\n    },\r\n// remove fruit add\r\n      removeFruit(indexToDelete) {\r\n      const fruitFiltered = this.smoothie.fruit.filter((item, index) => index !== indexToDelete);\r\n      this.smoothie.fruit = fruitFiltered;\r\n    },\r\n    onReset(e) {\r\n        e.preventDefault()\r\n        // Reset our Smothie values\r\n        this.smoothie.name = ''\r\n        this.smoothie.fruit = ['']\r\n        this.smoothie.liquids = ''\r\n        this.smoothie.proteins = ''\r\n\r\n      },\r\n    \r\n\r\n    }   \r\n       \r\n  }\r\n\r\n</script>\r\n"],"sourceRoot":"src/components"}]}