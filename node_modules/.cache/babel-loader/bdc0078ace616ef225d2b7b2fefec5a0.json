         :items=\"fruits\"\r\n                        label=\"Fruits\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                    <v-col cols=\"2\" sm=\"2\" align-self=\"center\">\r\n                      <v-btn :disabled=\"disabledAddFruit\" color=\"green\" icon text @click=\"addFruit\">\r\n                        <v-icon>add_box</v-icon>\r\n                      </v-btn>\r\n                    </v-col>\r\n                </v-row>\r\n                <div v-for=\"(item, i) in smoothie.fruit\" :key=\"`smoothie-fruit-${i}`\">\r\n                    <v-row \r\n                   class=\"mb-6\"\r\n                   justify=\"center\"\r\n                   v-if=\"i > 0\"\r\n                    >\r\n                   <v-col cols=\"8\" sm=\"8\">\r\n                     <p class=\"font-weight-bold\">Fruits:</p>\r\n                       <v-select\r\n                        v-model=\"smoothie.fruit[i]\"\r\n                        :items=\"fruits\"\r\n                        label=\"Fruits\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                    <v-col cols=\"2\" sm=\"2\" align-self=\"center\">\r\n                      <v-btn color=\"red\" icon text @click=\"removeFruit(i)\">\r\n                        <v-icon>clear</v-icon>\r\n                      </v-btn>\r\n                    </v-col>\r\n                </v-row>\r\n                  </div>\r\n                <v-row \r\n                   class=\"mb-6\"\r\n                   justify=\"center\"\r\n                 >\r\n                   <v-col cols=\"10\" sm=\"10\">\r\n                     <p class=\"font-weight-bold\">Liquids:</p>\r\n                       <v-select\r\n                        v-model=\"smoothie.liquids\"\r\n                        :items=\"liquids\"\r\n                        label=\"Liquids\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                </v-row>    \r\n                <v-row \r\n                   class=\"mb-6\"\r\n                   justify=\"center\"\r\n                 >\r\n                   <v-col cols=\"10\" sm=\"10\">\r\n                     <p class=\"font-weight-bold\">Proteins:</p>\r\n                       <v-select\r\n                        v-model=\"smoothie.proteins\"\r\n                        :items=\"proteins\"\r\n                        label=\"Proteins\"\r\n                        dense\r\n                       ></v-select>\r\n                    </v-col>\r\n                </v-row>    \r\n                <v-row \r\n                   class=\"mb-6 ml-6\"\r\n                   justify=\"lefth\"\r\n                   \r\n                 >\r\n                 <p class=\"font-weight-bold\">Taste:</p>\r\n                   <v-col cols=\"4\" sm=\"3\">\r\n                     \r\n                       <v-text-field\r\n                         value=\"Taste0%\"\r\n                         outlined\r\n                          readonly\r\n                      ></v-text-field>\r\n                   </v-col>\r\n                   <v-col cols=\"10\" sm=\"12\" class=\"text-center\">\r\n                     <v-btn class=\"ma-2\" outlined color=\"indigo\">Save it!</v-btn>\r\n                   </v-col>\r\n                </v-row>    \r\n                \r\n               </v-form>\r\n        </v-container>\r\n  </v-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n   \r\n   import {mapState} from 'vuex';\r\n \r\n  export default {\r\n    name: 'Create',\r\n\r\n    data: () => ({\r\n      valid: true,\r\n             smoothie:{\r\n              name : '',\r\n              fruit :[''],\r\n              liquids: '',\r\n              proteins:  ''\r\n\r\n      },\r\n      nameRules: [\r\n        v => !!v || 'Name is required',\r\n            ],\r\n      \r\n    \r\n      \r\n    }),\r\n    computed:{\r\n      ...mapState(['fruits','proteins','liquids']),\r\n      disabledAddFruit() {\r\n      let index = 0;\r\n      \r\n      const countFruits = this.smoothie.fruit.length;\r\n      if(countFruits > 0)\r\n        index = countFruits - 1;\r\n      if(!this.smoothie.fruit[index])\r\n        return true;\r\n      return false;\r\n    },\r\n\r\n    },\r\n    methods:{\r\n      addFruit() {\r\n      this.smoothie.fruit.push('');\r\n    },\r\n    removeFruit(indexToDelete) {\r\n      const fruitFiltered = this.smoothie.fruit.filter((item, index) => index !== indexToDelete);\r\n      this.smoothie.fruit = fruitFiltered;\r\n    },\r\n    }\r\n    \r\n    \r\n      \r\n  }\r\n\r\n</script>\r\n"],"sourceRoot":"src/components"}]}